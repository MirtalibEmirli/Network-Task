<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Margin="10" x:Name="ip" Height="50" Width="500" Background="Black" Foreground="White"/>
            <TextBox Margin="10" x:Name="port" Height="50" Width="500" Background="Black" Foreground="White"/>
            <Button Margin="10" x:Name="img" Height="50" Content="Image" Click="img_Click" />
            <Button  Margin="10" x:Name="send"  Height="50" Content="send" Click="send_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
